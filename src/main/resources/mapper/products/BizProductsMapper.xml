<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.youlai.boot.products.mapper.BizProductsMapper">

    <!-- 获取商品供应分页列表 -->
    <select id="getBizProductsPage" resultType="com.youlai.boot.products.model.vo.BizProductsVO">
        SELECT
                id,
                name,
                specification,
                origin,
                production_date,
                expiration_date,
                unit_price,
                description,
                image_preview,
                created_time,
                updated_time,
                created_by,
                updated_by
        FROM
            biz_products
        <where>
        </where>
    </select>

</mapper>
